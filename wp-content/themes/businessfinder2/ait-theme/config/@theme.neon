			label: Hide Empty Values
			type: on-off
			default: off
			help: "Hide empty contact information"
		addressHideGpsField:
			label: Hide GPS
			type: on-off
			default: off
			help: "Hide GPS information"
		addressWebNofollow:
			label: Nofollow Web Link
			type: on-off
			default: off
			help: "Use nofollow attribute in web link for SEO purposes"
