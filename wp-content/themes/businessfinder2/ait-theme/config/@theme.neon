general:
	title: General
	options:

		layoutType:
			label: Layout Type
			type: image-radio
			checked: layout-wide
			images:
				layout-wide: layout-wide.png
				layout-narrow: layout-narrow.png
			default:
				layout-wide: Wide Layout
				layout-narrow: Narrow Layout

		websiteWidth:
			label: Website Width
			type: range
			unit: px
			min: 1000
			max: 1800
			step: 50
			default: 1200

		leftSidebarWidth:
			label: Left Sidebar Width
			type: range
			unit: %
			min: 10.0
			max: 30.0
			step: 0.1
			default: 20.6

		rightSidebarWidth:
			label: Right Sidebar Width
			type: range
			unit: %
			min: 10.0
			max: 30.0
			step: 0.1
			default: 26.1

		sidebarGap:
			type: variable
			default: 4

		progressivePageLoading:
			label: Progressive Page Loading
			type: on-off
			default: off

		- section(title: 'Main Theme Colors and Backgrounds')

		favicon:
			label: Favicon
			type: image
			default: "/design/img/favicon.png"

		mainbg:
			label: Website Background
			type: background
			default:
				color: '#ffffff'
				opacity: 100%
				image: ''
				repeat: no-repeat
				position: top center
				scroll: scroll
			required: true

		pageBgColor:
			label: Page Background
			type: color
			default: '#ffffff'
			opacity: 100%
			required: true

		titColor:
			label: Titles
			type: color
			default: '#35cbc9'
			required: true

		txtColor:
			label: Text
			type: color
			default: '#777777'
			required: true

		lnkColor:
			label: Links
			type: color
			default: '#ff9c00'
			required: true

		hvrColor:
			label: Links Hover
			type: color
			default: '#f37e2a'
			required: true

		decColor:
			label: Decoration
			type: color
			default: '#9cd8d8'
			required: true

		secdecColor:
			label: Second Decoration
			type: color
			default: '#ff9c00'
			required: true

		featuredColor:
			label: Featured Items Color
			type: color
			default: '#e4f8f8'
			required: true
			help: "Decorative Color for the Featured Items"

		linesColor:
			label: Lines
			type: color
			opacity: 10%
			default: '#000000'
			required: true

		lightObj:
			label: Lighter Objects
			type: color
			default: '#f7f7f7'
			opacity: 100%
			required: true

		darkObj:
			label: Darker Objects
			type: color
			default: '#666666'
			opacity: 100%
			required: true

header:
	title: Header
	options:

#		headerType:
#			label: Header Type
#			type: image-radio-full
#			checked: header-one
#			images:
#				header-one: header-one.png
#				header-two: header-two.png
#			default:
#				header-one: Header One
#				header-two: Header Two


		logo:
			label: Website Logo
			type: image
			default: '/design/img/logo.png'

		logowidth:
			label: Logo width
			type: range
			unit: %
			min: 10.0
			max: 30.0
			step: 0.5
			default: 24.0
			help: "Reserved space for logo"

		menucollapse:
			label: Menu Collapse
			type: range
			unit: px
			min: 768
			max: 1920
			step: 10
			default: 970
			help: "Resolution when main menu is set under logo"

		headbg:
			label: Background
			type: background
			default:
				color: '#ffffff'
				opacity: 100%
				image: ''
				repeat: repeat
				position: center center
				scroll: scroll
			required: true

		topbarbg:
			label: Top Bar Background
			type: color
			default: '#f7f7f7'
			opacity: 100%

		topbarcolor:
			label: Top Bar Color
			type: color
			default: '#777777'

		stickyMenu:
			label: Sticky Menu
			type: on-off
			default: on

		displayHeaderResources:
			label: Display Header Resources
			type: on-off
			default: on

		headerResourcesButtonLink:
			label: Button Link
			type: posts
			cpt: page
			default: ""
			help: "Link used when current user is not logged in"

		userPanel:
			label: Display User Panel
			type: select
			selected: enable
			default:
				enable: Enable
				disable: Disable

		- section(id: userPanel-enable, title: 'User Panel Settings', help: "In case you don't want to provide additional info, leave following fields empty.")

		descLogin:
			label: Login Description
			type: text
			default: ''

		descRegister:
			label: Register Description
			type: text
			default: ''

		conditions:
			label: Terms & Conditions
			type: text
			default: ''

		helperUsername:
			label: Help text for Username field
			type: text
			help: "Description for additional information or GDPR."
			default: ''

		helperEmail:
			label: Help text for Email field
			type: text
			help: "Description for additional information or GDPR."
			default: ''

		headerPanelUseCaptcha:
			label: Use Captcha
			type: on-off
			default: on

		- section(title: 'Main Menu Colors and Backgrounds')

		menuColor:
			label: Menu Items
			type: color
			default: '#666666'

		menuColorBg:
			label: Menu Items Background
			type: color
			default: '#000000'
			opacity: 3%
			help: 'Background of menu item, leave empty for transparent'

		menuColorHover:
			label: Menu Items Hover
			type: color
			default: ""
			opacity: 100%
			help: 'Background of menu item on hover, leave empty for auto'

		menuColorAct:
			label: Active Menu Item
			type: color
			default: '#9cd8d8'
			help: 'Background of active menu item'

		menuDropBg:
			label: Dropdown Background
			type: color
			default: '#f7f7f7'
			help: 'Background of menu item dropdowns'

		menubg:
			label: Sticky Menu Background
			type: color
			default: '#ffffff'
			opacity: 100%
			image: ''
			repeat: repeat
			position: top center
			scroll: scroll

		- section(title: Custom JavaScript Code)

		customJsCode:
			label: JavaScript Code
			type: multiline-code
			default: ''

