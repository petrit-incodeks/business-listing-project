		mapDisplayAdministrativeShow:
			label: Display administratives
			type: on-off
			default: on
		mapDisplayRoadsShow:
			label: Display roads
			type: on-off
			default: on
		mapDisplayWaterShow:
			label: Display water
			type: on-off
			default: on
		mapDisplayPoiShow:
			label: Display poi
			type: on-off
			default: on
		- section(title: Address Options)
